<network>
  <name>{{ ansible_role_libvirtd.netname }}</name>
  <forward mode='nat'>
    <nat>
      <port start='1024' end='65535'/>
    </nat>
  </forward>
  <domain name='{{ ansible_role_libvirtd.domainname }}' localOnly='yes'/>
  <ip address='192.168.122.1' netmask='255.255.255.0'>
    <dhcp>
      <range start='192.168.122.128' end='192.168.122.254'/>
      <bootp file='pxelinux.0' server='192.168.122.125' /> 
    </dhcp>
  </ip>
</network>
